{
    "ParagraphTitled": "\\paragraph{${0}}",
    "BetweenParagraphs": "\n\\vspace{10pt}\n",
    "BetweenStructuralBlocks": "\n\n",

    "TextIndent": "\\par ${0}",
    "TextNoindent": "\\par\\noindent ${0}",
    "BetweenTexts": "\n",
    "BetweenBasicBlocks": "\n",

    "EquationReference": "\\eqref{${0}}",
    "BibliographyReference": "\\cite{${0}}",
    "DefaultReference": "\\ref{${0}}",

    "InlineFormula": "$${0}$",
    "NumberedFormula": "\\begin{equation}${reference}\n\\setlength\\abovedisplayskip{4pt}\\setlength\\belowdisplayskip{4pt}\n${content}\n\\end{equation}",
    "UnnumberedFormula": "\\begin{equation*}${reference}\n\\setlength\\abovedisplayskip{4pt}\\setlength\\belowdisplayskip{4pt}\n${content}\n\\end{equation*}",
    "MultilineFormula": "\\begin{aligned}\n${content}\n\\end{aligned}",

    "Figure": "\\begin{figure}[!htbp]\n\\centering\n\\vspace{-0.3em}\n${content}\n\\vspace{-0.3em}\n${caption}${reference}\\vspace{-0.7em}\n\\end{figure}",
    "DefaultImage": "\\subcaptionbox{${caption}}{\\includegraphics[width = ${size}\\linewidth]{${path}}}",
    "SingleImage": "\\includegraphics[width = ${size}\\linewidth]{${path}}",
    "Caption": "\\caption{${0}}",
    "BetweenImagesAndCaption": "\n",

    "InlineCode": "\\verb|${0}|",
    "DefaultCode": "\\begin{lstlisting}\n${0}\n\\end{lstlisting}",
    "NumberedList": "\\begin{enumerate}\n${0}\n\\end{enumerate}",
    "UnnumberedList": "\\begin{itemize}\n${0}\n\\end{itemize}",
    "Item": "\\item${reference} ${content}",
    "BetweenItems": "\n",
    "Table": "\\begin{table}[!htbp]\n\\centering\n\\begin{tabular}{${args}}\n${content}\n\\end{tabular}\n${caption}${reference}\n\\end{table}",
    "TableRowSeparator": "\\\\",
    "TableColumnSeparator": "&",
    "Cell": "${0}",
    "Emph": "\\emph{${0}}",
    "Bold": "\\textbf{${0}}",
    "Italic": "\\textit{${0}}",

    "Title": "\\title{${0}}",
    "Author": "\\author{${0}}",
    "Date": "\\date{${0}}",
    "MakeTitleAuthorDate": "\\maketitle",
    "Section": "\\section{${content}}${reference}",
    "Subsection": "\\subsection{${content}}${reference}",
    "Subsubsection": "\\subsubsection{${content}}${reference}",
    "UnnumberedSection": "\\section{${content}}${reference}",
    "UnnumberedSubsection": "\\subsection{${content}}${reference}",
    "UnnumberedSubsubsection": "\\subsubsection{${content}}${reference}",
    "Tableofcontents": "\\tableofcontents",
    "Newpage": "\\newpage",

    "Bibliography": "\\begin{thebibliography}{1}\n${0}\n\\end{thebibliography}",
    "BibItem": "\\bibitem{${reference}} ${content}",
    "BetweenBibItems": "\n",

    "Theorem": "\\begin{theorem}${reference}${content}\\end{theorem}",
    "Definition": "\\begin{definition}${reference}${content}\\end{definition}",
    "Lemma": "\\begin{lemma}${reference}${content}\\end{lemma}",
    "Corollary": "\\begin{corollary}${reference}${content}\\end{corollary}",
    "Proposition": "\\begin{proposition}${reference}${content}\\end{proposition}",
    "Proof": "\\begin{proof}${reference}${content}\\end{proof}"

}